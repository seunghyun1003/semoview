ction () {\r\n        return {\r\n            credentials : {\r\n                username : null,\r\n                password : null,\r\n            }\r\n        }\r\n    },\r\n    methods: {\r\n        login: function () {\r\n            axios({\r\n                method : 'post',\r\n                url : 'http://127.0.0.1:8000/api-token-auth/obtain_token/',\r\n                data : this.credentials,\r\n            })\r\n            .then(res => {\r\n                console.log(res)\r\n                // 로컬스토리지에 토큰 저장\r\n                localStorage.setItem('jwt', res.data.token)\r\n\r\n                \r\n                const token = localStorage.getItem('jwt')\r\n                const decoded = jwt_decode(token);\r\n\r\n                console.log(decoded)\r\n                this.setAuthUser(decoded)\r\n                \r\n                this.$router.push({ name : 'Home'})\r\n                this.$router.go();\r\n            })\r\n            .catch(err => {\r\n                console.log(err)\r\n            })\r\n        },\r\n        setAuthUser: function (decoded) {\r\n            this.$store.commit('setAuthUser', decoded);\r\n        }\r\n    },\r\n}\r\n</script>"]}]}