\r\n            </div>\r\n            <div v-for=\"review in reviewList\" :key=\"review.id\">\r\n                <div class=\"review-item-username\">{{review.user_username}}</div>\r\n                <div class=\"review-item-point\">{{review.point}}</div>\r\n                <div class=\"review-item-content\">{{review.reviewContents}}</div>\r\n            </div>\r\n        </div>\r\n        \r\n        <div v-else>\r\n            <div class=\"review-title\">연극은 어떠셨나요?</div>\r\n            \r\n        </div>\r\n    </div>\r\n\r\n    \r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport axios from 'axios'\r\n\r\nexport default {\r\n  name: 'Detail',\r\n  data() {\r\n    return {\r\n      stage: [],\r\n      reviewList: [],\r\n      isLogin: true,\r\n      isShow: false,\r\n    };\r\n  },\r\n  created: function() {\r\n    this.fetch_this_stage(); \r\n    this.fetch_all_review();\r\n  },\r\n  methods: {\r\n    fetch_this_stage: function () {\r\n        console.log(this.$route.params.contentId)\r\n      axios.get('http://127.0.0.1:8000/stages')\r\n      .then(res => {             \r\n        const index = this.$route.params.contentId-1\r\n        this.stage = res.data[index]\r\n      })\r\n      .catch(err => {\r\n        console.log(err)\r\n      })\r\n    },\r\n    fetch_all_review: function () {\r\n      const url = 'http://127.0.0.1:8000/reviews/'+this.$route.params.contentId\r\n      axios.get(url)\r\n      .then(res => {             \r\n        this.reviewList = res.data;\r\n      })\r\n      .catch(err => {\r\n        console.log(err)\r\n      })\r\n    },\r\n    writeReview: function() {\r\n        \r\n    },\r\n    onShown() {\r\n        // Focus the cancel button when the overlay is showing\r\n        this.$refs.cancel.focus()\r\n      },\r\n      onHidden() {\r\n        // Focus the show button when the overlay is removed\r\n        this.$refs.show.focus()\r\n      }\r\n    }\r\n}\r\n\r\n</script>\r\n\r\n<style scoped>\r\n  #stage-item{\r\n    border-bottom: 1px solid rgb(157, 157, 157);\r\n  }\r\n  .stage-title{\r\n    font-weight: bolder;\r\n    font-size: 1.2em;\r\n    text-align: left;\r\n  }\r\n  .stage-info{\r\n    padding: 1.6em 2em 1.8em;\r\n  }\r\n  #item-img{\r\n    min-width: 120px;\r\n    width: 120px;\r\n    height: 8em;\r\n    padding: 0 1em 0 0;\r\n  }\r\n  #item-img img{\r\n    width: 100%;\r\n    height: 100%;\r\n  }\r\n\r\n\r\n  #review-item{\r\n    text-align: left;\r\n    padding: 1em;\r\n  }\r\n  .review-item-header{\r\n    display: flex;\r\n    justify-content: space-between;\r\n  }\r\n  .review-title{\r\n    font-size: 1.1em;\r\n    font-weight: bold;\r\n    padding-bottom: 0.8em; \r\n  }\r\n  .review-write-btn > button{\r\n    background-color: rgb(231, 194, 43);\r\n    border: none;\r\n    color: black;\r\n  }\r\n  .review-item-username{\r\n    font-size: 1.1em;\r\n  }\r\n  .review-item-point{\r\n    font-size: 0.4em;\r\n  }\r\n  .review-item-content{\r\n    padding: 0.4em 0 1em 1.4em;\r\n    \r\n  }\r\n</style>\r\n"]}]}