IAAA,WAAA,EAAA,qBAAA,OAAA,EAAA;AACA,WAAA,MAAA,CAAA,MAAA,CAAA,aAAA,EAAA,OAAA;AACA;AA5BA;AAVA,CAAA","sourcesContent":["<template>\r\n    <div>\r\n        <h1>Login</h1>\r\n        <div>\r\n        <label for=\"username\">사용자 이름: </label>\r\n        <input type=\"text\" id=\"username\" v-model=\"credentials.username\">\r\n        </div>\r\n        <div>\r\n        <label for=\"password\">비밀번호: </label>\r\n        <input type=\"password\" id=\"password\" v-model=\"credentials.password\">\r\n        </div>\r\n\r\n        <button @click=\"login\">로그인</button>\r\n        <div>\r\n            <router-link :to=\"{name: 'Signup'}\">아직 회원이 아니신가요?</router-link>\r\n        </div>\r\n\r\n    </div>\r\n</template>\r\n\r\n<script>\r\nimport axios from 'axios'\r\nimport jwt_decode from \"jwt-decode\";\r\n\r\n// const SERVER_URL = process.env.VUE_APP_SERVER_URL\r\n\r\nexport default {\r\n    name: 'Login',\r\n    data: function () {\r\n        return {\r\n            credentials : {\r\n                username : null,\r\n                password : null,\r\n            }\r\n        }\r\n    },\r\n    methods: {\r\n        login: function () {\r\n            axios({\r\n                method : 'post',\r\n                url : 'http://127.0.0.1:8000/api-token-auth/obtain_token/',\r\n                data : this.credentials,\r\n            })\r\n            .then(res => {\r\n                console.log(res)\r\n                // 로컬스토리지에 토큰 저장\r\n                localStorage.setItem('jwt', res.data.token)\r\n\r\n                \r\n                const token = localStorage.getItem('jwt')\r\n                const decoded = jwt_decode(token);\r\n\r\n                console.log(decoded)\r\n                this.setAuthUser(decoded)\r\n                \r\n                this.$router.push({ name : 'Home'})\r\n                this.$router.go();\r\n            })\r\n            .catch(err => {\r\n                console.log(err)\r\n            })\r\n        },\r\n        setAuthUser: function (decoded) {\r\n            this.$store.commit('setAuthUser', decoded);\r\n        }\r\n    },\r\n}\r\n</script>"],"sourceRoot":"src/views"}]}