AAQA,KAVA;AAWA,IAAA,gBAAA,EAAA,4BAAA;AAAA;;AACA,UAAA,GAAA,GAAA,mCAAA,KAAA,MAAA,CAAA,MAAA,CAAA,SAAA;AACA,MAAA,KAAA,CAAA,GAAA,CAAA,GAAA,EACA,IADA,CACA,UAAA,GAAA,EAAA;AACA,QAAA,MAAA,CAAA,UAAA,GAAA,GAAA,CAAA,IAAA;AACA,OAHA,EAIA,KAJA,CAIA,UAAA,GAAA,EAAA;AACA,QAAA,OAAA,CAAA,GAAA,CAAA,GAAA;AACA,OANA;AAOA,KApBA;AAqBA,IAAA,cArBA,4BAqBA;AACA,UAAA,KAAA,OAAA,IAAA,KAAA,EAAA;AACA,QAAA,KAAA,CAAA,kBAAA,CAAA;AACA,OAFA,MAEA;AACA,aAAA,MAAA,GAAA,IAAA;AACA;AACA,KA3BA;AA4BA,IAAA,QA5BA,oBA4BA,KA5BA,EA4BA;AAAA;;AACA,MAAA,KAAA,CAAA,cAAA;;AACA,UAAA,KAAA,IAAA,CAAA,OAAA,KAAA,CAAA,EAAA;AACA,QAAA,KAAA,CAAA,aAAA,CAAA;AACA,OAFA,MAEA;AACA,aAAA,IAAA,CAAA,OAAA,GAAA,EAAA;AACA,aAAA,IAAA,CAAA,KAAA,GAAA,CAAA;AACA;;AACA,UAAA,GAAA,GAAA,iCAAA,KAAA,MAAA,CAAA,MAAA,CAAA,SAAA,GAAA,SAAA;AACA,MAAA,KAAA,CAAA,IAAA,CAAA,GAAA,EAAA;AACA,QAAA,OAAA,EAAA,CADA;AAEA,QAAA,QAAA,EAAA,KAAA,MAAA,CAAA,MAAA,CAAA,SAFA;AAGA,QAAA,KAAA,EAAA,KAAA,IAAA,CAAA,KAHA;AAIA,QAAA,cAAA,EAAA,KAAA,IAAA,CAAA;AAJA,OAAA,EAMA,IANA,CAMA,UAAA,GAAA,EAAA;AACA,QAAA,MAAA,CAAA,UAAA,CAAA,IAAA,CAAA,GAAA,CAAA,IAAA;;AACA,QAAA,OAAA,CAAA,GAAA,CAAA,GAAA,CAAA,IAAA;AACA,QAAA,MAAA,CAAA,IAAA,CAAA,KAAA,GAAA,CAAA;AACA,QAAA,MAAA,CAAA,IAAA,CAAA,OAAA,GAAA,EAAA;AACA,QAAA,MAAA,CAAA,MAAA,GAAA,KAAA;AACA,OAZA,EAaA,KAbA,CAaA,UAAA,GAAA,EAAA;AACA,QAAA,OAAA,CAAA,GAAA,CAAA,GAAA;AACA,OAfA;AAgBA,KArDA;AAsDA,IAAA,OAtDA,mBAsDA,KAtDA,EAsDA;AACA,MAAA,KAAA,CAAA,cAAA;AACA,WAAA,IAAA,CAAA,OAAA,GAAA,EAAA;AACA,WAAA,IAAA,CAAA,KAAA,GAAA,CAAA;AACA,MAAA,OAAA,CAAA,GAAA,CAAA,MAAA;AACA,WAAA,MAAA,GAAA,IAAA;AACA;AA5DA;AAzBA,CAAA","sourcesContent":["<template>\r\n  <div id=\"detail\">\r\n    <div id=\"stage-item\">\r\n        <div class=\"stage-title\">{{stage.stageTitle}}</div>\r\n        <div class=\"stage-info\">\r\n            <div id=\"item-img\"><img :src=\"stage.stageImglink\" alt /></div>\r\n        </div>\r\n    </div>\r\n    \r\n    <div id=\"review-item\">\r\n        <div v-if=\"isShow == false\">\r\n            <div class=\"review-item-header\">\r\n                <div class=\"review-title\">리뷰 ({{reviewList.length}}개)</div>\r\n                <div class=\"review-write-btn\">\r\n                    <button @click=\"showReviewForm()\">\r\n                        <b-icon icon=\"pencil-square\"></b-icon>\r\n                    </button>\r\n                </div>\r\n            </div>\r\n            <div v-for=\"review in reviewList\" :key=\"review.id\">created_at\r\n                <div class=\"review-item-username\">{{review.user_username}}</div>\r\n                <div class=\"review-item-point\">{{review.point}}</div>\r\n                <div class=\"review-item-content\">{{review.reviewContents}}</div>\r\n                <div class=\"review-item-point\">{{review.created_at}}</div>\r\n            </div>\r\n        </div>\r\n        \r\n        <div v-else>\r\n            <div class=\"review-title\">연극은 어떠셨나요?</div>\r\n            <b-form @submit=\"onSubmit\" @reset=\"onReset\">\r\n                <star-rating \r\n                    v-model=\"form.point\"\r\n                    v-bind:increment=\"1\"\r\n                    v-bind:max-rating=\"5\"\r\n                    inactive-color=\"white\"\r\n                    active-color=\"gold\"\r\n                    v-bind:star-size=\"40\"\r\n                ></star-rating>\r\n                <b-form-textarea\r\n                id=\"input-2\"\r\n                v-model=\"form.content\"\r\n                placeholder=\"내용을 입력해주세요.\"\r\n                required\r\n                ></b-form-textarea>\r\n                <div>\r\n                    <b-button type=\"reset\">취소</b-button>\r\n                    <b-button type=\"submit\" variant=\"primary\">완료</b-button>\r\n                </div>\r\n                </b-form>\r\n        </div>\r\n    </div>\r\n\r\n    \r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport axios from 'axios'\r\nimport StarRating from 'vue-star-rating'\r\n\r\nexport default {\r\n  name: 'Detail',\r\n  components: {\r\n    StarRating\r\n  },\r\n  data() {\r\n    return {\r\n      stage: [],\r\n      reviewList: [],\r\n      isLogin: true,\r\n      isShow: false,\r\n      form: {\r\n          point: 0,\r\n          content: '',\r\n      },\r\n    };\r\n  },\r\n  created: function() {\r\n    const token = localStorage.getItem('jwt')\r\n    if(token){\r\n      this.isLogin = true\r\n    }\r\n    this.fetch_this_stage(); \r\n    this.fetch_all_review();\r\n  },\r\n  methods: {\r\n    fetch_this_stage: function () {\r\n      axios.get('http://127.0.0.1:8000/stages')\r\n      .then(res => {             \r\n        const index = this.$route.params.contentId-1\r\n        this.stage = res.data[index]\r\n      })\r\n      .catch(err => {\r\n        console.log(err)\r\n      })\r\n    },\r\n    fetch_all_review: function () {\r\n      const url = 'http://127.0.0.1:8000/reviews/'+this.$route.params.contentId\r\n      axios.get(url)\r\n      .then(res => {             \r\n        this.reviewList = res.data;\r\n      })\r\n      .catch(err => {\r\n        console.log(err)\r\n      })\r\n    },\r\n    showReviewForm() {\r\n        if (this.isLogin == false) {\r\n            alert('로그인이 필요한 서비스입니다.')\r\n        } else {\r\n            this.isShow = true\r\n        }\r\n    },\r\n    onSubmit(event) {\r\n        event.preventDefault()\r\n        if (this.form.content === 0) {\r\n            alert('별점을 선택해주세요.')\r\n        } else {\r\n            this.form.content = ''\r\n            this.form.point = 0\r\n        }\r\n        const url = 'http://127.0.0.1:8000/stage='+this.$route.params.contentId+'/create'\r\n        axios.post(url, {\r\n            user_id : 1,\r\n            stage_id : this.$route.params.contentId,\r\n            point : this.form.point,\r\n            reviewContents : this.form.content,\r\n        })\r\n        .then(res => {       \r\n            this.reviewList.push(res.data)   \r\n            console.log(res.data) \r\n            this.form.point = 0  \r\n            this.form.content = ''\r\n            this.isShow = false\r\n        })\r\n        .catch(err => {\r\n            console.log(err)\r\n        })\r\n    },\r\n    onReset(event) {\r\n        event.preventDefault()\r\n        this.form.content = ''\r\n        this.form.point = 0\r\n        console.log(\"작성취소\")\r\n        this.isShow = true\r\n    },\r\n  }\r\n}\r\n\r\n</script>\r\n\r\n<style scoped>\r\n  #stage-item{\r\n    border-bottom: 1px solid rgb(157, 157, 157);\r\n  }\r\n  .stage-title{\r\n    font-weight: bolder;\r\n    font-size: 1.2em;\r\n    text-align: left;\r\n  }\r\n  .stage-info{\r\n    padding: 1.6em 2em 1.8em;\r\n  }\r\n  #item-img{\r\n    min-width: 120px;\r\n    width: 120px;\r\n    height: 8em;\r\n    padding: 0 1em 0 0;\r\n  }\r\n  #item-img img{\r\n    width: 100%;\r\n    height: 100%;\r\n  }\r\n\r\n\r\n  #review-item{\r\n    text-align: left;\r\n    padding: 1em;\r\n  }\r\n  .review-item-header{\r\n    display: flex;\r\n    justify-content: space-between;\r\n  }\r\n  .review-title{\r\n    font-size: 1.1em;\r\n    font-weight: bold;\r\n    padding-bottom: 0.8em; \r\n  }\r\n  .review-write-btn > button{\r\n    background-color: rgb(231, 194, 43);\r\n    border: none;\r\n    color: black;\r\n  }\r\n  .review-item-username{\r\n    font-size: 1.1em;\r\n  }\r\n  .review-item-point{\r\n    font-size: 0.4em;\r\n  }\r\n  .review-item-content{\r\n    padding: 0.4em 0 1em 1.4em;\r\n    \r\n  }\r\n\r\n  #review-item > div > div.vue-star-rating {\r\n    justify-content: center;\r\n  }\r\n</style>\r\n"],"sourceRoot":"src/views"}]}