AAA,GAAA,KAAA;AACA;AACA,SAbA,EAcA,KAdA,CAcA,UAAA,GAAA,EAAA;AACA,UAAA,OAAA,CAAA,GAAA,CAAA,GAAA;AACA,UAAA,KAAA,CAAA,OAAA,GAAA,qBAAA;AACA,SAjBA;AAkBA,OAnBA,MAmBA;AACA,aAAA,OAAA,GAAA,4BAAA;AACA;AACA,KAxBA;AAyBA,IAAA,MAAA,EAAA,kBAAA;AAAA;;AACA,UAAA,KAAA,WAAA,CAAA,QAAA,IAAA,EAAA,IAAA,KAAA,WAAA,CAAA,KAAA,IAAA,EAAA,IACA,KAAA,WAAA,CAAA,SAAA,IAAA,EADA,IACA,KAAA,WAAA,CAAA,SAAA,IAAA,EADA,EACA;AACA,YAAA,KAAA,UAAA,IAAA,IAAA,EAAA;AACA,UAAA,KAAA,CAAA;AACA,YAAA,MAAA,EAAA,MADA;AAEA,YAAA,GAAA,EAAA,yCAFA;AAGA,YAAA,IAAA,EAAA,KAAA;AAHA,WAAA,CAAA,CAKA,IALA,CAKA,UAAA,GAAA,EAAA;AACA,YAAA,OAAA,CAAA,GAAA,CAAA,GAAA,CAAA,MAAA,EAAA,gBAAA,EADA,CAEA;;AACA,YAAA,MAAA,CAAA,OAAA,CAAA,IAAA,CAAA;AAAA,cAAA,IAAA,EAAA;AAAA,aAAA;AACA,WATA,EAUA,KAVA,CAUA,UAAA,GAAA,EAAA;AACA,YAAA,OAAA,CAAA,GAAA,CAAA,GAAA;AACA,YAAA,MAAA,CAAA,OAAA,GAAA,gBAAA;AACA,WAbA;AAcA,SAfA,MAeA;AACA,UAAA,KAAA,CAAA,kBAAA,CAAA;AACA;AACA,OApBA,MAoBA;AACA,QAAA,KAAA,CAAA,gBAAA,CAAA;AACA;AACA;AAjDA;AAdA,CAAA","sourcesContent":["<template>\r\n  <div id=\"signup\">\r\n    <div class=\"errorMsg\">{{message}}</div>\r\n    <h1><strong>Signup</strong> </h1>\r\n    <div>\r\n      <label for=\"username\"></label>\r\n      <input type=\"text\" id=\"username\" v-model=\"credentials.username\" placeholder=\"USERNAME\" > \r\n      <button @click=\"checkUsername(credentials.username)\">중복확인</button>\r\n    </div>\r\n    <div>\r\n      <label for=\"email\"></label>\r\n      <input type=\"email\" id=\"email\" v-model=\"credentials.email\" placeholder=\"EMAIL\" >\r\n    </div>\r\n    <div>\r\n      <label for=\"password\"></label>\r\n      <input type=\"password\" id=\"password1\" v-model=\"credentials.password1\" placeholder=\"PASSWORD\" >\r\n    </div>\r\n    <div>\r\n      <label for=\"passwordConfirmation\"></label>\r\n      <input type=\"password\" id=\"password2\" v-model=\"credentials.password2\" placeholder=\"PASSWORD CHECK\" >\r\n    </div>\r\n    <button @click=\"signup\">회원가입</button>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport axios from 'axios'\r\n\r\n// const SERVER_URL = process.env.VUE_APP_SERVER_URL\r\n\r\nexport default {\r\n  name: 'Signup',\r\n  data: function () {\r\n    return {\r\n      message: '',\r\n      ckUsername: false,\r\n      credentials : {\r\n        username : null,\r\n        email : null,\r\n        password1 : null,\r\n        password2 : null,\r\n      }\r\n    }\r\n  },\r\n  methods: {\r\n    checkUsername: function (username) {\r\n      if(username != null) {\r\n        axios ({\r\n          method : 'post',\r\n          url : 'http://127.0.0.1:8000/ckeck/username',\r\n          data: username,\r\n        })\r\n          .then(res => {\r\n            if (res.data) {\r\n              this.message = \"사용가능한 아이디입니다.\"\r\n              this.ckUsername = true\r\n            } else {\r\n              this.message = \"다른 아이디를 사용해주세요.\"\r\n              this.ckUsername = false\r\n            }\r\n          })\r\n          .catch(err => {\r\n            console.log(err);\r\n            this.message = \"실패) 다른 아이디로 시도해주세요.\"\r\n          })\r\n      } else {\r\n        this.message = \"중복확인 실패) USERNAME을 입력해주세요.\";\r\n      }\r\n    },\r\n    signup: function () {\r\n      if (this.credentials.username != '' && this.credentials.email != '' &&\r\n            this.credentials.password1 != '' && this.credentials.password2 != ''){\r\n        if (this.ckUsername == true) {\r\n          axios ({\r\n            method : 'post',\r\n            url : 'http://127.0.0.1:8000/rest-auth/signup/',\r\n            data: this.credentials,\r\n          })\r\n            .then(res => {\r\n              console.log(res.status, 'Success Signup')\r\n              // 회원가입에 성공하면 로그인 페이지로 보내기\r\n              this.$router.push({ name : 'Signin'})\r\n            })\r\n            .catch(err => {\r\n              console.log(err);\r\n              this.message = \"실패) 다시 시도해주세요.\"\r\n            })\r\n        } else {\r\n          alert('ID 중복 확인이 필요합니다.')\r\n        }\r\n      } else {\r\n        alert('모든 정보를 입력해주세요.')\r\n      }\r\n    }\r\n  }\r\n}\r\n</script>\r\n\r\n<style scoped>\r\n#signup{\r\n    display: flex;\r\n    flex-flow: column;\r\n    justify-content : center;\r\n    align-items: center;\r\n    height:100%;\r\n    padding-bottom: 10em;\r\n}\r\n.errorMsg{\r\n    font-size: 0.8em;\r\n    margin-bottom: 2em;\r\n    color:salmon;\r\n}\r\n#signup > h1 {\r\n    margin-bottom: 1em;\r\n}\r\ninput[type=\"text\"],\r\ninput[type=\"password\"],\r\ninput[type=\"email\"]{\r\ntext-align:center;\r\nmargin-bottom: 0.6em;\r\n}\r\n#signup > button {\r\n    background-color: rgb(85, 85, 85);\r\n    border:gray;\r\n    color:white;\r\n    padding: 0.4em 0.6em;\r\n    border-radius: 0.4em;\r\n    margin: 0.8em 0 1.5em 0;\r\n}\r\n</style>"],"sourceRoot":"src/views"}]}