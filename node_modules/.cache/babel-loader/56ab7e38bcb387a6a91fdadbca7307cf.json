late>\n  <div id=\"home\">\n    <div class=\"page-title\">\n      예매랭킹\n    </div>\n    <div>        \n      <div class=\"\" v-for=\"(stage, id) in stageList\" :key=\"id\">\n          <div>{{stage.id}} </div> \n          <div>{{stage.stageTitle}} </div> \n          <div>{{stage.stageImglink}}</div> \n      </div> \n      <div class=\"card-footer pb-0 pt-3\">\n            <jw-pagination :items=\"exampleItems\" @changePage=\"onChangePage\"></jw-pagination>\n        </div>\n    </div>\n    <div class=\"overflow-auto\">\n      <b-table\n      id=\"my-table\"\n      :items=\"stageList\"\n      :per-page=\"perPage\"\n      :current-page=\"currentPage\"\n      small>\n      </b-table>\n      <b-pagination\n      v-model=\"currentPage\"\n      :per-page=\"perPage\"\n      aria-controls=\"my-table\">\n      </b-pagination>\n    </div>\n  </div>\n</template>\n\n<script>\nimport axios from 'axios'\n\nconst exampleItems = [...Array(150).keys()].map(i => ({ id: (i+1), name: 'Item ' + (i+1) }));\n\nexport default {\n  name: 'Home',\n  data() {\n    return {\n      stageList : [],\n      exampleItems,\n    }\n  },\n  created: function() {\n    this.fetch_all_stage(); \n  },\n  methods: {\n    fetch_all_stage: function () {\n      console.log('fetch_all_stage')\n      axios.get('http://127.0.0.1:8000/stages')\n      .then(res => {    \n        console.log(res)            \n        this.stageList = res.data;\n        console.log(this.stageList)\n      })\n      .catch(err => {\n        console.log(err)\n      })\n    },\n    onChangePage(stageList) {\n      // update page of items\n      this.stageList = stageList;\n    }\n  }\n}\n</script>\n\n<style>\n  #home{\n    overflow-y: auto;\n  }\n  .page-title{\n    font-weight: bolder;\n    font-size: 1.2em;\n    text-align: left;\n  }\n</style>\n"],"sourceRoot":"src/views"}]}